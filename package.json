    "@types/jsrsasign": "^8.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/urlencode": "^1.1.1",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "https-proxy-agent": "^5.0.0",
    "mocha": "^8.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "ts-loader": "^8.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1",
    "uglify-js": "^3.10.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "base64url": "^3.0.1",
    "bignumber.js": "^8.0.1",
    "body-parse": "^0.1.0",
